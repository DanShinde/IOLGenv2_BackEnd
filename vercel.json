  {
    "version": 2,
    "builds": [
      {
        "src": "IOLGenv2_BackEnd/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.12" }
      }],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "IOLGenv2_BackEnd/wsgi.py"
      }
    ]
  }
  